--[[
SENSATION.CC - Fixed Mouse Event Error
--]]

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local CoreGui = game:GetService("CoreGui")

-- A. Конфигурация
local Config = {
    BunnyHop = {Enabled = false, Speed = 1.2},
    AntiAim = {
        Enabled = false,
        Yaw = {Enabled = false, Value = 0},
        BodyYaw = {Enabled = false, Value = 0},
        HeadYaw = {Enabled = false, Value = 0},
        Pitch = {Enabled = false, Value = 0},
        FakeYaw = {Enabled = false, Value = 0},
        Spinbot = {Enabled = false, Speed = 3},
        HeadFake = {Enabled = false, Intensity = 1}
    }
}

-- B. Создание GUI
local mainGUI = nil
local isMenuVisible = false

local function CreateMainGUI()
    if mainGUI then mainGUI:Destroy() end
    
    mainGUI = Instance.new("ScreenGui")
    mainGUI.Name = "SensationCC"
    mainGUI.Enabled = false
    mainGUI.Parent = CoreGui
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 450, 0, 500)
    mainFrame.Position = UDim2.new(0.5, -225, 0.5, -250)
    mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    mainFrame.BorderSizePixel = 1
    mainFrame.BorderColor3 = Color3.fromRGB(60, 60, 60)
    mainFrame.Parent = mainGUI
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 35)
    title.Text = "SENSATION.CC - ANTI-AIM SETTINGS"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    title.Font = Enum.Font.GothamBold
    title.TextSize = 14
    title.Parent = mainFrame
    
    local contentFrame = Instance.new("Frame")
    contentFrame.Size = UDim2.new(1, -20, 1, -45)
    contentFrame.Position = UDim2.new(0, 10, 0, 40)
    contentFrame.BackgroundTransparency = 1
    contentFrame.Parent = mainFrame
    
    local yOffset = 0
    local function nextY() yOffset = yOffset + 30 return yOffset - 30 end
    
    -- Создаем элементы управления
    CreateCheckbox("BunnyHop Enabled", 0, nextY(), contentFrame, 
        function(enabled) Config.BunnyHop.Enabled = enabled end)
    
    CreateSlider("BunnyHop Speed", 0, nextY(), contentFrame, 0.5, 3, 1.2,
        function(value) Config.BunnyHop.Speed = value end)
    
    nextY()
    
    CreateCheckbox("Anti-Aim Enabled", 0, nextY(), contentFrame,
        function(enabled) Config.AntiAim.Enabled = enabled end)
    
    CreateCheckbox("Yaw", 0, nextY(), contentFrame,
        function(enabled) Config.AntiAim.Yaw.Enabled = enabled end)
    CreateSlider("Yaw Value", 0, nextY(), contentFrame, -180, 180, 0,
        function(value) Config.AntiAim.Yaw.Value = value end)
    
    CreateCheckbox("Spinbot", 0, nextY(), contentFrame,
        function(enabled) Config.AntiAim.Spinbot.Enabled = enabled end)
    CreateSlider("Spinbot Speed", 0, nextY(), contentFrame, 0.1, 10, 3,
        function(value) Config.AntiAim.Spinbot.Speed = value end)
    
    return mainGUI
end

-- C. Исправленная функция создания чекбокса
function CreateCheckbox(text, x, y, parent, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 0, 25)
    frame.Position = UDim2.new(0, x, 0, y)
    frame.BackgroundTransparency = 1
    frame.Parent = parent
    
    local checkbox = Instance.new("TextButton")
    checkbox.Size = UDim2.new(0, 20, 0, 20)
    checkbox.Position = UDim2.new(0, 0, 0, 2)
    checkbox.Text = ""
    checkbox.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    checkbox.BorderSizePixel = 1
    checkbox.BorderColor3 = Color3.fromRGB(100, 100, 100)
    checkbox.Parent = frame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -25, 1, 0)
    label.Position = UDim2.new(0, 25, 0, 0)
    label.Text = text
    label.TextColor3 = Color3.fromRGB(200, 200, 200)
    label.BackgroundTransparency = 1
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Font = Enum.Font.Gotham
    label.TextSize = 12
    label.Parent = frame
    
    local checked = false
    
    checkbox.MouseButton1Click:Connect(function()
        checked = not checked
        checkbox.BackgroundColor3 = checked and Color3.fromRGB(0, 150, 255) or Color3.fromRGB(60, 60, 60)
        callback(checked)
    end)
    
    return frame
end

-- D. Исправленная функция создания слайдера
function CreateSlider(text, x, y, parent, min, max, default, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 0, 40)
    frame.Position = UDim2.new(0, x, 0, y)
    frame.BackgroundTransparency = 1
    frame.Parent = parent
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 0, 15)
    label.Text = text .. ": " .. default
    label.TextColor3 = Color3.fromRGB(200, 200, 200)
    label.BackgroundTransparency = 1
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.Font = Enum.Font.Gotham
    label.TextSize = 11
    label.Parent = frame
    
    local sliderTrack = Instance.new("Frame")
    sliderTrack.Size = UDim2.new(1, 0, 0, 6)
    sliderTrack.Position = UDim2.new(0, 0, 0, 20)
    sliderTrack.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    sliderTrack.BorderSizePixel = 0
    sliderTrack.Parent = frame
    
    local sliderFill = Instance.new("Frame")
    sliderFill.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
    sliderFill.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    sliderFill.BorderSizePixel = 0
    sliderFill.Parent = sliderTrack
    
    local sliderButton = Instance.new("TextButton")
    sliderButton.Size = UDim2.new(0, 12, 0, 12)
    sliderButton.Position = UDim2.new((default - min) / (max - min), -6, 0.5, -6)
    sliderButton.Text = ""
    sliderButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sliderButton.BorderSizePixel = 0
    sliderButton.Parent = sliderTrack
    
    local dragging = false
    
    local function updateValue(xPos)
        local relativeX = math.clamp(xPos / sliderTrack.AbsoluteSize.X, 0, 1)
        local value = min + (max - min) * relativeX
        value = math.floor(value * 100) / 100
        
        sliderFill.Size = UDim2.new(relativeX, 0, 1, 0)
        sliderButton.Position = UDim2.new(relativeX, -6, 0.5, -6)
        label.Text = text .. ": " .. value
        
        callback(value)
    end
    
    -- ИСПРАВЛЕННАЯ СТРОКА: правильное название события
    sliderButton.MouseButton1Down:Connect(function() 
        dragging = true 
    end)
    
    -- ИСПРАВЛЕННАЯ СТРОКА: правильное название события
    sliderTrack.MouseButton1Down:Connect(function() 
        local mousePos = UserInputService:GetMouseLocation()
        updateValue(mousePos.X - sliderTrack.AbsolutePosition.X)
    end)
    
    UserInputService.InputEnded:Connect(function(input) 
        if input.UserInputType == Enum.UserInputType.MouseButton1 then 
            dragging = false 
        end 
    end)
    
    RunService.Heartbeat:Connect(function()
        if dragging then
            local mousePos = UserInputService:GetMouseLocation()
            updateValue(mousePos.X - sliderTrack.AbsolutePosition.X)
        end
    end)
    
    return frame
end

-- E. Система переключения меню
local function SetupToggleSystem()
    UserInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.Delete then
            isMenuVisible = not isMenuVisible
            mainGUI.Enabled = isMenuVisible
        end
    end)
end

-- F. BunnyHop система
local function SetupBunnyHop()
    RunService.Heartbeat:Connect(function()
        if not Config.BunnyHop.Enabled then return end
        
        local player = Players.LocalPlayer
        if not player then return end
        
        local character = player.Character
        if character and character:FindFirstChild("Humanoid") then
            local humanoid = character.Humanoid
            if humanoid.MoveDirection.Magnitude > 0 then
                humanoid.Jump = true
            end
        end
    end)
end

-- G. Anti-Aim система
local function SetupAntiAim()
    RunService.Heartbeat:Connect(function()
        if not Config.AntiAim.Enabled then return end
        
        local player = Players.LocalPlayer
        if not player then return end
        
        local character = player.Character
        if not character then return end
        
        local humanoid = character:FindFirstChild("Humanoid")
        local root = character:FindFirstChild("HumanoidRootPart")
        
        if not humanoid or not root then return end
        
        humanoid.AutoRotate = false
        
        -- Spinbot
        if Config.AntiAim.Spinbot.Enabled then
            local spinAngle = tick() * Config.AntiAim.Spinbot.Speed
            local spinCFrame = CFrame.new(root.Position) * CFrame.Angles(0, spinAngle, 0)
            character:SetPrimaryPartCFrame(spinCFrame)
        end
        
        -- Yaw
        if Config.AntiAim.Yaw.Enabled then
            local yawCFrame = CFrame.new(root.Position) * CFrame.Angles(0, math.rad(Config.AntiAim.Yaw.Value), 0)
            character:SetPrimaryPartCFrame(yawCFrame)
        end
    end)
end

-- H. Инициализация
local function Initialize()
    CreateMainGUI()
    SetupToggleSystem()
    SetupBunnyHop()
    SetupAntiAim()
    print("SENSATION.CC Fixed Version Loaded! Press DEL to toggle menu")
end

-- Запуск
local success, err = pcall(Initialize)
if not success then
    print("Error: " .. err)
end
